{
  "name": "flarum-lang/de",
  "type": "flarum-extension",
  "description": "Flarum language pack German.",
  "keywords": ["locale", "de", "German"],
  "license": "MIT",
  "require": {
    "flarum/core": "^0.1.0-beta.10"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "0.1.x-dev"
    },
    "flarum-extension": {
      "title": "German",
      "icon": {
        "image": "icon.svg",
        "backgroundColor": "#00247d",
        "backgroundSize": "cover",
        "backgroundPosition": "center"
      }
    },
    "flarum-locale": {
      "code": "de",
      "title": "German"
    }
  }
}
